{
  "format_version": "1.8.0",
  "animations": {
    "animation.warg.idle": {
      "loop": true,
      "bones": {
        "torso_main": {
          "position": [0, "math.sin(query.life_time * 100) * 0.2", 0],
          "rotation": ["math.cos(query.life_time * 50) * 1.5", 0, 0]
        },
        "neck_base": {
          "rotation": ["math.sin(query.life_time * 50) * -2", 0, 0]
        },
        "tail_proximal": {
          "rotation": [0, "math.cos(query.life_time * 80) * 5", 0]
        },
        "tail_distal": {
          "rotation": [0, "math.cos(query.life_time * 80 - 20) * 8", 0]
        },
        "jaw_lower": {
          "rotation": ["5 + math.sin(query.life_time * 120) * 2", 0, 0]
        }
      }
    },
    "animation.warg.walk": {
      "loop": true,
      "anim_time_update": "query.modified_distance_moved",
      "bones": {
        "leg_front_left_thigh": {
          "rotation": ["math.cos(query.anim_time * 30) * 30", 0, 0]
        },
        "leg_front_left_shin": {
          "rotation": ["math.max(0, math.cos(query.anim_time * 30) * 20)", 0, 0]
        },
        "leg_back_right_thigh": {
          "rotation": ["math.cos(query.anim_time * 30) * 30", 0, 0]
        },
        "leg_back_right_shin": {
           "rotation": ["math.max(0, math.cos(query.anim_time * 30) * 20)", 0, 0]
        },
        "leg_front_right_thigh": {
          "rotation": ["math.cos(query.anim_time * 30 + 180) * 30", 0, 0]
        },
        "leg_front_right_shin": {
           "rotation": ["math.max(0, math.cos(query.anim_time * 30 + 180) * 20)", 0, 0]
        },
        "leg_back_left_thigh": {
          "rotation": ["math.cos(query.anim_time * 30 + 180) * 30", 0, 0]
        },
        "leg_back_left_shin": {
           "rotation": ["math.max(0, math.cos(query.anim_time * 30 + 180) * 20)", 0, 0]
        },
        "torso_main": {
          "position": [0, "math.abs(math.sin(query.anim_time * 30)) * 1", 0],
          "rotation": [0, 0, "math.cos(query.anim_time * 15) * 2"]
        },
        "tail_proximal": {
          "rotation": [-10, "math.cos(query.anim_time * 30) * 10", 0]
        }
      }
    },
    "animation.warg.attack": {
      "loop": "hold_on_last_frame",
      "animation_length": 0.5,
      "bones": {
        "head_cranium": {
          "rotation": {
            "0.0": [0, 0, 0],
            "0.2": [-15, 0, 0],
            "0.3": [10, 0, 0],
            "0.5": [0, 0, 0]
          }
        },
        "jaw_lower": {
          "rotation": {
            "0.0": [0, 0, 0],
            "0.2": [35, 0, 0],
            "0.4": [0, 0, 0]
          }
        },
        "neck_base": {
          "rotation": {
            "0.0": [0, 0, 0],
            "0.2": [-10, 0, 0],
            "0.3": [15, 0, 0]
          }
        }
      }
    },
    "animation.warg.look_at_target": {
      "loop": true,
      "bones": {
        "head_cranium": {
          "rotation": ["query.target_x_rotation", "query.target_y_rotation", 0]
        },
        "neck_base": {
          "rotation": ["query.target_x_rotation * 0.5", "query.target_y_rotation * 0.5", 0]
        }
      }
    }
  }
}