{
  "format_version": "1.8.0",
  "animations": {
    "animation.hobbit.idle": {
      "loop": true,
      "bones": {
        "torso_core": {
          "rotation": ["math.cos(query.life_time * 90) * 1.5", 0, 0],
          "position": [0, "math.sin(query.life_time * 90) * 0.1", 0]
        },
        "head_skin": {
          "rotation": ["math.sin(query.life_time * 45) * 2", "math.cos(query.life_time * 50) * 3", 0]
        },
        "arm_right_upper": {
          "rotation": [0, 0, "2 + math.cos(query.life_time * 90) * 2"]
        },
        "arm_left_upper": {
          "rotation": [0, 0, "-2 - math.cos(query.life_time * 90) * 2"]
        },
        "jacket_tails": {
          "rotation": ["5 + math.sin(query.life_time * 90) * 2", 0, 0]
        }
      }
    },
    "animation.hobbit.walk": {
      "loop": true,
      "anim_time_update": "query.modified_distance_moved",
      "bones": {
        "torso_core": {
          "rotation": [5, "math.cos(query.anim_time * 30) * 2", 0],
          "position": [0, "math.abs(math.cos(query.anim_time * 30)) * 0.5", 0]
        },
        "leg_right_breeches": {
          "rotation": ["math.cos(query.anim_time * 45) * 40", 0, 0]
        },
        "foot_right_bare": {
          "rotation": ["math.clamp(math.cos(query.anim_time * 45) * 10, -10, 0)", 0, 0]
        },
        "leg_left_breeches": {
          "rotation": ["math.cos(query.anim_time * 45 + 180) * 40", 0, 0]
        },
        "foot_left_bare": {
          "rotation": ["math.clamp(math.cos(query.anim_time * 45 + 180) * 10, -10, 0)", 0, 0]
        },
        "arm_right_upper": {
          "rotation": ["math.cos(query.anim_time * 45 + 180) * 30", 0, 5]
        },
        "arm_left_upper": {
          "rotation": ["math.cos(query.anim_time * 45) * 30", 0, -5]
        },
        "jacket_tails": {
          "rotation": ["15 + math.abs(math.sin(query.anim_time * 90)) * 10", 0, 0]
        }
      }
    },
    "animation.hobbit.attack": {
      "loop": "hold_on_last_frame",
      "animation_length": 0.5,
      "bones": {
        "torso_core": {
          "rotation": {
            "0.0": [0, 0, 0],
