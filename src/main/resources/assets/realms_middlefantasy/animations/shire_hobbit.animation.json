        "arm_right_upper": {
          "rotation": {
            "0.0": [0, 0, 0],
            "0.1": [-40, 0, 0],
            "0.3": [-100, -20, 0],
            "0.5": [0, 0, 0]
          }
        },
        "arm_right_forearm": {
          "rotation": {
            "0.0": [0, 0, 0],
            "0.1": [-30, 0, 0],
            "0.3": [0, 0, 0]
          }
        }
      }
    }
  }
}